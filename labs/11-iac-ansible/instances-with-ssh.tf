resource "google_compute_instance" "host-1" {
  project      = "PROJECT_ID"
  name         = "host-1"
  machine_type = "n1-standard-1"
  zone         = "us-west1-c"
  boot_disk {
    initialize_params {
      image = "debian-cloud/debian-11"
    }
  }
  network_interface {
    network = "default"
    access_config {
    }
  }
}